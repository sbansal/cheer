document.getElementById('transactions-container').innerHTML = "<%= j (render 'transactions') %>"
document.getElementById('tx-label').innerHTML = "Displaying <%= pluralize(@transactions.size, 'transaction') %>"
document.getElementById('transactions-by-categories').innerHTML = " <%= j (render 'transactions_by_categories', transactions_by_category: @transactions_by_category)%>"
document.getElementById('transactions-by-merchants').innerHTML = " <%= j (render 'transactions_by_merchants')%>"
<% if @duplicates_count > 0 %>
  document.getElementById('toggle-duplicates').classList.remove('hide')
<% else %>
  document.getElementById('toggle-duplicates').classList.add('hide')
<% end %>
