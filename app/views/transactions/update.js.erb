var oldNode = document.getElementById("transaction-detail-<%= @transaction.id%>")
var newNode = document.createElement("template")
newNode.innerHTML = "<%= j (render @transaction) %>"
oldNode.replaceWith(newNode.content.firstChild)
var txRow = document.getElementById("tx-<%= @transaction.id %>")
if (txRow) {
  txRow.innerHTML = "<%= j (render 'transaction_summary', transaction: @transaction) %>"
}
